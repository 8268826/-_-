#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,i;
    a=20;b=10;c=60;
    i=0;
    while(true)
    {
        if(a+b*i>c)
        {
            cout<<i-1;
            break;
        }
        i++;
    }
}

#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a;
    bool b=false;
    a=1;
    for(int i=0;i<100;i++)
        for(int j=0;j<100;j++)
        {
            if(i*3+4*j==a)
            {
                b=true;
                cout<<i<<endl<<j<<endl;
                break;
            }
        }
    if(!b)
        cout<<0<<endl<<0;
}


#include <iostream>
using namespace std;

int vippro(int k,int n)
{
	if (n == 1) return 1;
	return k*vippro(k, n - 1);
}

int main()
{
	int m, n, k = 0;
	cin >> m >> n;
	for (int i = 1; i <= m; i++)
	{
		k += i;
	}
	n += 1;
	cout << vippro(k,n) << endl;
}
