#include <bits/stdc++.h>
using namespace std;

void DeleteX(string& str)
{
    int j;
    for(int i=0;i<str.length();i++)
    {
        if(str[i]>='A'&&str[i]<='z')
        {
            j = i;
			while (str[j+1]!='\0')
			{
				str[j] = str[j + 1];
				j++;
			}
			str[j] = '\0';
			i--;
        }
    }
}

