#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,i;
    a=20;b=10;c=60;
    i=0;
    while(true)
    {
        if(a+b*i>c)
        {
            cout<<i-1;
            break;
        }
        i++;
    }
}

#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a;
    bool b=false;
    a=1;
    for(int i=0;i<100;i++)
        for(int j=0;j<100;j++)
        {
            if(i*3+4*j==a)
            {
                b=true;
                cout<<i<<endl<<j<<endl;
                break;
            }
        }
    if(!b)
        cout<<0<<endl<<0;
}


#include <iostream>
using namespace std;

int vippro(int k,int n)
{
	if (n == 1) return 1;
	return k*vippro(k, n - 1);
}

int main()
{
	int m, n, k = 0;
	cin >> m >> n;
	for (int i = 1; i <= m; i++)
	{
		k += i;
	}
	n += 1;
	cout << vippro(k,n) << endl;
}

#include <iostream>
#define A 3
#define B 18
using namespace std;

void Gan_Ghe(int a[][B])
{
	a[0][0] = 1;
	a[1][0] = 2;
	a[2][0] = 53;
	for (int j = 1; j < B; j++)
	{
		a[0][j] = a[0][j - 1] + 2;
		a[1][j] = a[1][j - 1] + 2;
		if (j % 2 != 0)
		{
			a[2][j] = a[2][j - 1] + 1;
		}
		else
		{
			a[2][j] = a[2][j - 1] - 3;
		}
	}
}

void Xac_Dinh_Ghe(int n,int a[][B],int x[],int &sum)
{
	for (int i = 0; i < A; i++)
	{
		for (int j = 0; j < B; j++)
		{
			for (int k = 0; k < n; k++)
			{
				if (x[k] == a[i][j])
				{
					a[i][j] = 0;
				}
			}
		}
	}
	for (int i = 0; i < B; i++)
	{
		if (a[0][i] == 0)
		{
			if (i % 2 == 0 && a[0][i + 1] == 0 && a[1][i] == 0 && a[1][i + 1] == 0 && a[2][i] == 0 && a[2][i + 1] == 0)
			{
				sum++;
				a[0][i] = 1;
			}
			else if (i % 2 != 0 && a[0][i - 1] == 0 && a[1][i] == 0 && a[1][i - 1] == 0 && a[2][i] == 0 && a[2][i - 1] == 0)
			{
				sum++;
				a[0][i] = 1;
			}
		}
	}
}

int main()
{
	int a[A][B], n;
	int sum = 0;
	Gan_Ghe(a);
	cout << "Number of seats: "; cin >> n;
	int* x = new int[n];
	cout << "INPUT: " << endl;
	for (int i = 0; i < n; i++)
	{
		cin >> x[i];
	}
	Xac_Dinh_Ghe(n, a, x, sum);
	cout <<"OUTPUT: "<< sum;
	delete[]x;
	return 0;
}
