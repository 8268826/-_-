#include<stdio.h>
#include<string.h>
int main()
{
    char a[100];
    char b[30];
    char c[30];
    int j = 0, maxlength = 0;

    gets(a);

    for(int i = 0; i <= strlen(a); i++){
        if(a[i] != 32 && a[i] != '\0'){
	    b[j++] = a[i];
	}else{
            b[j] = '\0';
            if(strlen(b) > maxlength){
                strcpy(c, b);
                maxlength = strlen(b);
            }
            j = 0;
        }
    }puts(c); // output - result
    j=0;
    for(int i = 0; i <= strlen(a); i++)
    {
        if(a[i] != 32 && a[i] != '\0'){
	    b[j++] = a[i];
	}else{
            b[j] = '\0';
            if(strlen(b) == maxlength){
                strcpy(c, b);
                puts(c);
            }
            j = 0;
        }
    }
    return 0;
}


#include <iostream>
#include <string>
using namespace std;

int main()
{
	string str;
	char c;
	getline(cin, str);
	cin >> c;
	int j;
	for (int i = 0; i < str.length(); i++)
	{
		if (str[i]==c)
		{
			j = i;
			while (str[j+1]!='\0')
			{
				str[j] = str[j + 1];
				j++;
			}
			str[j] = '\0';
			i--;
		}
	}
	cout << str;

}
