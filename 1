{
    for(int i=0;i<str.length();i++)
    {

    }
}

int main()
{
    fstream input_1, input_2,output;
    string str_1,str_2;
    input_1.open("txt_1.txt",ios::out);
    input_2.open("txt_2.txt",ios::out);
    getline(cin,str_1);
    DeleteX(str_1);
    input_1<<str_1;
    getline(cin,str_2);
    input_2<<str_2;
    return 0;
}
