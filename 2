#include <iostream>
using namespace std;

int main()
{
	int m, n;
	cin >> m >> n;
	for (int i = m; i < n; i++)
	{
		for (int j = i+1; j < n; j++)
		{
			float a = sqrt(i * i + j * j);
			if (a<=n&&a>j&&a-(int)a==0)
			{
				cout << i<<" " << j << " " << a << endl;
			}
		}
	}
	return 0;
}


#include <iostream>
using namespace std;

bool check(int &x)
{
	int sum = 0;
	for (int i = 1; i < x; i++)
	{
		if (x%i==0)
		{
			sum += i ;
		}
		
	}
	if (sum==x)
	{
		return 1;
	}
}

int main()
{
	int m, n, sum = 0;
	cin >> m >> n;
	if (m<=2)
		m = 3;
	for (int i = m; i <= n; i++)
	{
		if (check(i)==1)
		{
			cout << i << " ";
		}
	}
	return 0;
}

#include <iostream>
using namespace std;

bool check(int n,int m)
{
	int k=1,x=1;
	while (true)
	{
		if (n==k)
		{
			return 1;
		}
		else
		{
			x *= 10;
			k = m % x;
		}
		if (k>n)
		{
			return 0;
		}
	}
}

int main()
{
	int m, n, sum = 0;
	cin >> m >> n;
	for (int i = m; i < n; i++)
	{
		for (int j = i; j < n; j++)
		{
			if (i*i==j&&check(i,j)==1)
			{
				cout << i << "," << j << ";";
			}
		}
	}
	return 0;
}
